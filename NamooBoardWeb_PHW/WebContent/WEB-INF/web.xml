<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>NamooBoardWeb_PHW</display-name>

	<servlet>
		<servlet-name>JoinServlet</servlet-name>
		<jsp-file>/WEB-INF/views/user/join.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>JoinServlet</servlet-name>
		<url-pattern>/user/join.do</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<jsp-file>/WEB-INF/views/user/login.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/user/loginForm.do</url-pattern>
	</servlet-mapping>

	
	<servlet>
		<servlet-name>BoardRegistServlet</servlet-name>
		<jsp-file>/WEB-INF/views/board/boardregist.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>BoardRegistServlet</servlet-name>
		<url-pattern>/board/boardregist.xhtml</url-pattern>
	</servlet-mapping>	
	
	
	<servlet>
		<servlet-name>WriteServlet</servlet-name>
		<jsp-file>/WEB-INF/views/board/write.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>WriteServlet</servlet-name>
		<url-pattern>/board/write.xhtml</url-pattern>
	</servlet-mapping>	
</web-app>